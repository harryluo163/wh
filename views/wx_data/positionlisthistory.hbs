<script language="javascript">
    $(function(){
        //判断显示类型
        var displaytype = "mt4";
        if(displaytype == 'mt4'){
            $("#actmt4").addClass("active");
            $("#actey").removeClass("active");
            $("#tab-product-mt4").addClass("active");
            $("#tab-order-open-option").removeClass("active");
            $("#type").attr("value",'mt4');
        }else{
            $("#actey").addClass("active");
            $("#actmt4").removeClass("active");
            $("#tab-order-open-option").addClass("active");
            $("#tab-product-mt4").removeClass("active");
        }

        $("#tabMt4").click(function(){
            $("#type").attr("value",'mt4');
        });
        $("#tabEy").click(function(){
            $("#type").attr("value",'ey');
        });


    });
    function showDetail(orderNo)
    {
        window.location='/wx_data/positionlisthistoryddetail?t='+orderNo;
    }

</script>

