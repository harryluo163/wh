

<div class="navbar-fixed-top">
    <div class="row ijy-modal-dialog-full-title">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ijy-col-nopadding">
            <div class="ijy-modal-dialog-full-title-left" onclick="historyBack()"><img src="/images/leftgo.png" alt=""> </div>
            <div class="ijy-modal-dialog-full-title-middle">历史订单</div>
            <div class="ijy-modal-dialog-full-title-right"></div>
        </div>
    </div>
</div>


<div class="row pd0-15 ijy-common-total mt50">

</div>


<div class="ijy-option ijy-order-histroy-d">
    <ul id="myTab" class="nav nav-tabs">
        <li class="col-lg-6 col-md-6 col-sm-6 col-xs-6 active" id="actmt4"><a href="#tab-product-mt4" data-toggle="tab" id="tabMt4">
            MT4订单</a>
        </li>
        <!--<li class="col-lg-6 col-md-6 col-sm-6 col-xs-6" id="actey"><a href="#tab-order-open-option" data-toggle="tab" id="tabEy">期权订单</a></li>-->
    </ul>

    <div class="tab-content">
        <!-- 商品tab -->
        <div role="tabpanel" class="tab-pane active" id="tab-product-mt4">

            <div class="ijy-modal-dialog-full-content">
                <div class="row ijy-order-history-title">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ijy-col-nopadding">
                        <span class="ijy-order-history-title-name">商品</span><span class="ijy-order-history-title-closelots">数量</span><span>盈亏</span>
                    </div>
                </div>
                <input type="hidden" id="mt4pageid" value="2">
                <div class="row" id="mt4container" style=" margin-bottom:100px;">
                {{#each data}}
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ijy-col-nopadding"
                             onclick="showDetail('{{symbol}}_{{record.volume}}_{{record.open_price}}_{{record.order}}_{{totime  record.open_time}}_{{record.close_price}}_{{totime record.close_time}}_{{record.sl}}_{{record.tp}}_{{record.profit}}_{{record.taxes}}')">
                            <div class="ijy-order-history-item">
                                <div class="ijy-order-history-item-top"><span class="ijy-order-history-item-productname">{{symbol}}<span class="ijy-order-history-item-isbuy"><span class="ijy-color-up">&nbsp</span></span></span>
                                    <span class="ijy-order-history-item-closelots">{{record.volume}}</span><span class="ijy-order-history-item-profit">
                                                        <span class="ijy-color-up">{{record.profit}}</span>
                                                        </span></div>
                                <div class="ijy-order-history-item-mt4orderid">开仓时间：{{totime  record.open_time}}　 开仓价格：{{  record.open_price}} </div>
                                <div class="ijy-order-history-item-mt4orderid">平仓价格：{{record.close_price}}</div>
                            </div>
                        </div>
                {{/each}}

                </div>

            </div>

        </div>


    </div>
</div>



